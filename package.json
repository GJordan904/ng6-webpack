{
  "name": "ng6-webpack",
  "version": "1.0.0",
  "description": "My custom Webpack, Angular 6 Starter. SSR ready using ngUniversal and Express. Angular Material for UI.",
  "scripts": {
    "build": "webpack --env.env=dev --env.platform=web",
    "dev": "webpack-dev-server --env.env=dev --env.platform=web",
    "dev:server": "webpack --env.env=prod --env.platform=server --watch",
    "build:prod": "webpack --env.env=prod --env.platform=web",
    "build:ssr": "webpack --env.env=prod --env.platform=ssr",
    "build:server": "webpack --env.env=prod --env.platform=server",
    "build:all": "npm run build:prod && npm run build:ssr && npm run build:server",
    "deploy": "npm run build:prod -- --env.deploy --env.deployUrl= && npm run build:ssr && npm run build:server",
    "start": "node ./dist/server/main.js"
  },
  "keywords": [
    "angular",
    "webpack"
  ],
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/platform-server": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@nguniversal/common": "~6.0.0",
    "@nguniversal/express-engine": "~6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "~6.0.0",
    "bootstrap": "^4.1.1",
    "core-js": "~2.5.6",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-optimizer": "~0.6.3",
    "@angular-devkit/core": "~0.6.3",
    "@angular/compiler-cli": "~6.0.2",
    "@angular/language-service": "~6.0.2",
    "@ngtools/webpack": "^6.0.3",
    "@types/autoprefixer": "^6.7.3",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/express": "^4.16.0",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "~10.1.1",
    "@types/webpack": "^4.4.0",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/webpack-sources": "^0.1.4",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.5",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "less-loader": "^4.0.5",
    "license-webpack-plugin": "~1.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylus-loader": "^3.0.1",
    "ts-loader": "~4.3.0",
    "ts-node": "~6.0.3",
    "tsconfig-paths-webpack-plugin": "~3.1.3",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "~1.2.5",
    "url-loader": "~1.0.1",
    "webpack": "~4.8.3",
    "webpack-cli": "~2.1.3",
    "webpack-dev-server": "~3.1.4",
    "webpack-merge": "~4.1.2",
    "webpack-node-externals": "~1.7.2",
    "webpack-sources": "latest"
  },
  "author": "Grant Jordan",
  "license": "MIT"
}
